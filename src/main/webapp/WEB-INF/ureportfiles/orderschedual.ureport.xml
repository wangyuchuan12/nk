	(@rowNO := @rowNo+1 ) AS rowno,
	a.problemparts_id,
	a.problemparts_expressnumber,
	a.problemparts_type,
	a.problemparts_createdate,
	b.CREATE_DATE_,
	b.STATE_,
	b.OWNER_
FROM
	tapp_problemparts AS a,
	UFLO_HIS_TASK AS b,
	tbase_staffinfo AS c,
	(select @rowNO :=0) e
WHERE
	a.problemparts_id = b.BUSINESS_ID_
	AND b.OWNER_ = c.staff_username
	AND b.CREATE_DATE_ = '2018-05-24 00:00:00'
	AND c.staff_name = '人工客服1号'	
ORDER BY a.problemparts_createdate DESC]]></sql><field name="rowno"/><field name="problemparts_id"/><field name="problemparts_expressnumber"/><field name="problemparts_type"/><field name="problemparts_createdate"/><field name="CREATE_DATE_"/><field name="STATE_"/><field name="OWNER_"/></dataset></datasource><paper type="A4" left-margin="90" right-margin="90"
