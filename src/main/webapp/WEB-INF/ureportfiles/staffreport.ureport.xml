  if(param('staffName')==null||param('staffId')==null||param('staffMobil')==null){
  	return "SELECT
	c.staff_name,d.CREATE_DATE_,COUNT(2) as item_count
FROM
	tbase_roleinfo AS a,
	tbase_sysrltn AS b,
	tbase_staffinfo AS c,
	UFLO_HIS_TASK As d
WHERE
	b.sysrltn_type = 1 
AND a.id = 2
AND c.staff_id = b.sysrltn_assistid
AND a.id = b.sysrltn_mainid
AND c.staff_username = d.ASSIGNEE_
GROUP BY c.staff_name,d.CREATE_DATE_;"
  }else{
  return "SELECT
	c.staff_name,d.CREATE_DATE_,COUNT(2) as item_count
FROM
	tbase_roleinfo AS a,
	tbase_sysrltn AS b,
	tbase_staffinfo AS c,
	UFLO_HIS_TASK As d
WHERE
	b.sysrltn_type = 1 
AND a.id = 2
AND c.staff_id = b.sysrltn_assistid
AND a.id = b.sysrltn_mainid
AND c.staff_username = d.ASSIGNEE_
AND c.staff_name = :staffName
AND c.staff_id = :staffId
AND c.staff_mobil = :staffMobil
AND d.CREATE_DATE_ = :createDate
GROUP BY c.staff_name,d.CREATE_DATE_;"
  }
}]]></sql><field name="staff_name"/><field name="CREATE_DATE_"/><field name="item_count"/><parameter name="staffName" type="String" default-value="人工客服2号"/><parameter name="staffId" type="Integer" default-value="10003"/><parameter name="staffMobil" type="String" default-value="15966332255"/><parameter name="createDate" type="Date" default-value=""/></dataset></datasource><paper type="A4" left-margin="90" right-margin="90"
